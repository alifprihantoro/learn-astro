{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "importsNotUsedAsValues": "error",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@configs/*": [
        "src/configs/*"
      ]
    }
  }
}
